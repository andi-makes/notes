{{#*inline "content"}}

<style>
    {{embed "embed/index.css"}}
</style>

{{#each note}}
<article class="card">
    <h1><a href="/{{ path }}">{{ note_title filename }}</a></h1>
    <p class="author">by {{ note_author filename }} | {{ note_date filename }}</p>
    {{ note_description filename }}
</article>
{{/each}}

{{/inline}}

